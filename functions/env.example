# Development Environment Configuration
# Copy this file to .env and modify as needed

# Enable development utilities (wipe, seed, reset database)
# Set to "true" to enable dev utilities, "false" or unset to disable
ENABLE_DEV_UTILITIES=true

# Node environment
# Set to "development" to enable dev utilities automatically
NODE_ENV=development

# Firebase configuration
# These are typically set by Firebase Functions automatically
# FIREBASE_PROJECT_ID=your-project-id
# FIREBASE_REGION=us-central1 